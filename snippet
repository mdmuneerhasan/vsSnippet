		  "int power(int a,int p){",
		  "    if(p==0){",
		  "        return 1;",
		  "    }",
		  "    int ans=power(a,p/2)%MOD;",
		  "    ans=ans*ans;",
		  "    ans%=MOD;",
		  "    if(p&1){",
		  "        ans=ans*a;",
		  "        ans%=MOD;",
		  "    }",
		  "    return ans;",
		  "}",
